# Mobile App Environment Variables
# Copy this file to .env and fill in real values

# Stripe publishable key (required)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_key_here

# Google Maps API key (required for location features)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_key_here

# Backend API URL (required) — must include /api suffix
EXPO_PUBLIC_API_URL=http://localhost:3000/api

# Socket.IO server URL (required) — base URL without /api
EXPO_PUBLIC_SOCKET_URL=http://localhost:3000
