{
  "expo": {
    "name": "FIXED",
    "slug": "fixed-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "fixed",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fixed.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fixed.app"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.fixed.app",
          "enableGooglePay": true
        }
      ],
      "expo-secure-store",
      "expo-router"
    ],
    "extra": {
      "apiUrl": "https://radiosymmetrical-jeniffer-acquisitively.ngrok-free.dev/api",
      "environment": "development",
      "enableLogs": true,
      "eas": {
        "projectId": "d6938f3f-e8b0-4b3f-9ddd-d781ef02754a"
      }
    }
  }
}
